{
  "manifest_version": 3,
  "name": "url-shortcut",
  "version": "1.0",
  "description": "ショートカット文字列で特定のURLを開けるようにします",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png"
  },
  "action": {
     "default_popup": "popup/popup.html",
     "default_icon": {
        "16": "icons/icon-16.png",
        "32": "icons/icon-32.png"
     }
  },
  "options_page": "option/option.html",
  "permissions": ["tabs", "storage"]
}